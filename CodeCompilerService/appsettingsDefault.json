{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.EventLog", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "EventLog",
        "Args": {
          "source": "CodeCompilerService",
          "restrictedToMinimumLevel": "Information",
          "manageEventSource": true,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\LOGS\\CodeCompilerService.log",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ServiceOptions": {
    "Interval": "3000",
    "MaxThreads": 4,
    "InternalBufferSize": "8192",
    "SendMessagesToManager": true,
    "SendMessagesToManagerAboutBeingAlive": false,
    "SendMessagesPort": "3055"
  },
  "CodeCompilerLibOptions": {
    "InputPath": "",
    "OutputPath": "",
    "BuildToConsoleApp": false
  }
}